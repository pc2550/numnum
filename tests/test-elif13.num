int cond(bool b)
{
  int x;
  if (false)
    x = 42;
  else if (b) /* because this is an if statement whose condition evaluates to true, the below elif statement is skipped */
    x = 95;
  elif (b)
    x = 423;
  else
    x = 500;
  return x;
}

int main()
{
 print(cond(true));
 print(cond(false));
 return 0;
}
