int main()
{
  string path;
  string path2;
  int i;
  int j;
  float sum;
  float temp;
  byte[3][600][400] a;
  byte[3][600][400] b;
  path = "./cat-stripped.ppm";
  path2 = "./cat-check-flip.ppm";
  read(path, a);
  for (i = 0 ; i < 400 ; i = i + 1) {
    for (j = 0; j < 600 ; j = j + 1 ) {
      b[0][j][i] = a[0][j][400-i-1];
      b[1][j][i] = a[1][j][400-i-1];
      b[2][j][i] = a[2][j][400-i-1];
    }
  }
  write(path2, b);
  return 0;
}
